_gp4()
{
	local cur prev opts
	COMPREPLY=()
	cur="${COMP_WORDS[COMP_CWORD]}"
	prev="${COMP_WORDS[COMP_CWORD-1]}"
#	opts=`gp4 --help | grep "valid commands:" | awk ' { for ( i=3; i <= NF; i++) print $i }' | sed 's/,//'`
	opts="clone rebase submit"

	COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
	return 0
}
complete -F _gp4 gp4
